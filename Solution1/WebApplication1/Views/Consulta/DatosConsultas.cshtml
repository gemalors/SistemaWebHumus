<!-- Modal para agregar categoria -->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Agregar categoría </h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <!--formulario-->
                <div class="card">

                    <form>
                        <div class="card-body">

                            @*<input type="hidden" class="form-control" id="IDlibro" name="IDlibro" value="@ViewBag.Idlibro1">*@
                            @*<input type="hidden" class="form-control" id="Idproceso" name="Idproceso" value="ViewBag.Idp7">*@
                            <div class="form-group row">
                                <label for="fname"
                                       class="col-sm-3 text-end control-label col-form-label text-info"><b class="text-danger">* </b>Categoria</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="DetalleCategoria" name="Detallecategoria" required="" placeholder="Ingrese categoría...">
                                </div>
                            </div>

                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
