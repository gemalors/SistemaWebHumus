        document.getElementById('Idpreg').value = IDpregunta;
        document.getElementById('descripcion').value = Descripcion;
        document.getElementById('leyendasup').value = LeyendaSuperior;

        document.getElementById('idtipopreg').value = Idtipopregunta;

        if (Obligatorio == true) {
            document.getElementById('obligatorio1').checked = true;
        } else {
            document.getElementById('obligatorio2').checked = true;
        }


        if (TiposOpciones == "Opciones y respuestas") {
            document.getElementById('especificar1').checked = true;
        }
        if (TiposOpciones == "Solo respuestas") {
            document.getElementById('especificar2').checked = true;
        }
        if (TiposOpciones == "No aplica") {
            document.getElementById('especificar3').checked = true;
        }

    }





    function EditPreguntaAbierta(IDpregunta, IDpreguntaAbierta,Idtipodato) {


        document.getElementById('Idpregt1').value = IDpregunta;
        document.getElementById('IdpregAbierta').value = IDpreguntaAbierta;
        document.getElementById('idtipodato').value = Idtipodato;


    }




    function EditOpcionPregunta(IDopcionPreguntaSeleccion, descripOpcpreg) {



        document.getElementById('Idopcionpreg').value =IDopcionPreguntaSeleccion
        document.getElementById('descripciont2').value = descripOpcpreg;
        //document.getElementById('identificadort2').value = IdentificadorOpcionPregunta;


    }




    function EditOpcionRespPregunta(IDrespuestaopcion, DescripcionOpcionRespuesta) {



        document.getElementById('Idrespopcion').value = IDrespuestaopcion
        document.getElementById('descripciont3').value = DescripcionOpcionRespuesta;



    }





    function EditPregObservaciones(IDpreguntaObservaciones, Detallepregobservacion, Leyendainferior, Respuestas) {



        document.getElementById('Idpregob').value = IDpreguntaObservaciones
        document.getElementById('detalleob').value = Detallepregobservacion;
        document.getElementById('leyendaob').value = Leyendainferior;


        if (Respuestas == 1) {
            document.getElementById('respuesta1').checked = true;
        } else {
            document.getElementById('respuesta2').checked = true;
        }



    }







    function EliminarPregunta(IDpregunta) {
        var Idc =@ViewBag.Idc;
        var Idp=@ViewBag.Idpreg;


        Swal.fire({
            title: '¿Estás seguro?',
            text: "¿Quieres eliminar esta pregunta?",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Sí, Eliminar!',
            cancelButtonText:'Cancelar'
        }).then((result) => {
            if (result.isConfirmed) {

                 var data = {

                     'Idpreg': IDpregunta
        }
        $.ajax({

            url: "@Url.Action("EliminarPregunta", "Cuestionario")",
            type: 'POST',
            dataType: 'json',
            data: data,
            success: function (obj) {

                window.location.href = "../Cuestionario/GestionarPreguntas?Idcuestionario=" + Idc + "&Idpreg=" + Idp + "";
            }


        })


            }
        })



        }



