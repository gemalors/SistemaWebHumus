<!-- Modal para editar datos de proceso -->
<div class="modal fade" id="exampleModal2" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Editar datos de tipo de proceso</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <!--formulario-->
                <div class="card">
                    @using (Html.BeginForm("EditarTipoProceso", "Proceso", FormMethod.Post, new { @class = "form-horizontal" }))
                    {


                    <div class="card-body">

                        <input type="hidden" class="form-control" id="Idtipoproceso" name="Idtipoproceso">
                        <div class="form-group row">
                            <label for="lname" class="col-sm-3 text-end control-label col-form-label text-info">
                                <b class="text-danger">* </b>
                                Identificador de tipo de proceso:
                            </label>
                            <div class="col-sm-9">
                                <input type="number" class="form-control" id="ident" name="ident" required="" placeholder="0" disabled>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="lname" class="col-sm-3 text-end control-label col-form-label text-info">
                                <b class="text-danger">* </b>
                                Número de tipo de proceso:
                            </label>
                            <div class="col-sm-9">
                                <input type="number" class="form-control" id="numero" name="numero" required="" placeholder="0">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="fname"
                                   class="col-sm-3 text-end control-label col-form-label text-info"><b class="text-danger">* </b>Detalle de tipo de proceso:</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="detalletipospro" name="detalletipospro" required="" placeholder="Ej. Recepción de Libro">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="fname"
                                   class="col-sm-3 text-end control-label col-form-label text-info"><b class="text-danger">* </b>Descripción de tipo de proceso:</label>
                            <div class="col-sm-9">
                                <textarea class="form-control" id="descripcion" name="descripcion" required="" placeholder="Ej. Se receptan documentos de inicio de proceso."></textarea>


                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="lname" class="col-sm-3 text-end control-label col-form-label text-info">
                                <b class="text-danger">* </b>
                                % de Progreso:
                            </label>
                            <div class="col-sm-9">
                                <input type="number" class="form-control" id="progreso" name="progreso" required="" placeholder="0">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="lname"
                                   class="col-sm-3 text-end control-label col-form-label text-info"><b class="text-danger">* </b>Tiempo de duración:</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="duracionproceso" name="duracionproceso" required="" placeholder="Ej. 10 días">
                            </div>
                        </div>




                    </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
                            <button type="submit" class="btn btn-info">Guardar</button>
                        </div>
                    }
                </div>
                <!--end formulario-->
            </div>

        </div>
    </div>
</div>





