<script>

  

    function Validar(IDtiposprocesos, Detalletipospro, Progreso, Duracionproceso, Descripciontipoproceso, Numeroprocesos,Identificador) {

        document.getElementById('Idtipoproceso').value = IDtiposprocesos;
        document.getElementById('detalletipospro').value = Detalletipospro;

        document.getElementById('duracionproceso').value = Duracionproceso;
        document.getElementById('progreso').value = Progreso;
        document.getElementById('descripcion').value = Descripciontipoproceso;
        document.getElementById('numero').value = Numeroprocesos;
        document.getElementById('ident').value = Identificador;





    }





    function eliminar(IDtiposprocesos) {


        Swal.fire({
            title: '¿Estás seguro?',
            text: "¿Quieres eliminar este tipo de proceso?",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Sí, Eliminar!',
            cancelButtonText:'Cancelar'
        }).then((result) => {
            if (result.isConfirmed) {

                 var data = {

                'Idtipoproceso': IDtiposprocesos
        }
        $.ajax({

            url: "@Url.Action("EliminarTipoProceso", "Proceso")",
            type: 'POST',
            dataType: 'json',
            data: data,
            success: function (obj) {

                window.location.href = "@Url.Action("GestionarProcesos")";
            }


        })


            }
        })



    }




</script>








