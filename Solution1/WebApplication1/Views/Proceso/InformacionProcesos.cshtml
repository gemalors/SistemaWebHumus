<script>

   


    function EditarEstado(IDEstadosTiposproceso, DetalleEstados, VRequerimientosAutor, VEnviarProcesoRevision, VEnviarRevisionReversado, VRequerimientosAdmin, VEnviarResultados, VIniciarSiguienteProceso, VAsignarEvaluadores, VEnviarEvaluacionPares, VPublicacionLibro) {
        document.getElementById('idestado').value = IDEstadosTiposproceso;
        document.getElementById('detalle').value = DetalleEstados;

        if (VRequerimientosAutor == true) {
            document.getElementById('vra1').checked = true;
        } else {
            document.getElementById('vra2').checked = true;
        }

        if (VEnviarProcesoRevision == true) {
            document.getElementById('er1').checked = true;
        } else {
            document.getElementById('er2').checked = true;
        }

        if (VEnviarRevisionReversado == true) {
            document.getElementById('epr1').checked = true;
        } else {
            document.getElementById('epr2').checked = true;
        }

        if (VRequerimientosAdmin == true) {
            document.getElementById('vrad1').checked = true;
        } else {
            document.getElementById('vrad2').checked = true;
        }
        
        if (VEnviarResultados == true) {
            document.getElementById('eresult1').checked = true;
        } else {
            document.getElementById('eresult2').checked = true;
        }

        if (VIniciarSiguienteProceso == true) {
            document.getElementById('inp1').checked = true;
        } else {
            document.getElementById('inp2').checked = true;
        }

        if (VAsignarEvaluadores == true) {
            document.getElementById('ae1').checked = true;
        } else {
            document.getElementById('ae2').checked = true;
        }

        if (VEnviarEvaluacionPares == true) {
            document.getElementById('epares1').checked = true;
        } else {
            document.getElementById('epares2').checked = true;
        }

        if (VPublicacionLibro == true) {
            document.getElementById('pl1').checked = true;
        } else {
            document.getElementById('pl2').checked = true;
        }


    }







    function Validar3(IDrequerimiento, Detallerequerimiento, Descripcionrequerimiento, IDtipodato, Emisor, respuesta, Visibleadmin, Visibleautor, Visibleevaluador, Obligatorio, ResultadoProceso) {

        document.getElementById('Idrequerimiento1').value = IDrequerimiento;
        document.getElementById('detallerequerimiento1').value = Detallerequerimiento;
 
        document.getElementById('descripcionrequerimiento1').value = Descripcionrequerimiento;
        document.getElementById('tipodato1').value = IDtipodato;
     /*   document.getElementById('emisor').value = Emisor;*/
/*        window.alert("Bienvenido a nuestro sitio web "+Emisor);*/

        if (Emisor=="Administrador") {
            document.getElementById('emisor1').checked=true;
        }
        if (Emisor == "Autor") {
            document.getElementById('emisor2').checked = true;
        }
        if (Emisor == "Evaluador") {
            document.getElementById('emisor3').checked = true;
        }
        if (respuesta == "Si") {
            document.getElementById('respS').checked = true;
        }
