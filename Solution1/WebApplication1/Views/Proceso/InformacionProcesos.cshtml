<!-- Modales -->
<!-- Modal para editar requerimientos de un proceso -->
<div class="modal fade" id="exampleModal3" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Datos de requerimientos para proceso</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <!--formulario-->
                <div class="card">
                    @using (Html.BeginForm("EditRequerimientoProceso", "Proceso", FormMethod.Post, new { @class = "form-horizontal" }))
                    {

                        

                    <div class="card-body">
                        <input type="hidden" class="form-control" id="Idp1" name="Idp1" value="@ViewBag.Idp">

                        <input type="hidden" class="form-control" id="Idrequerimiento1" name="Idrequerimiento1">
                        <div class="form-group row">
                            <label for="fname"
                                   class="col-sm-3 text-end control-label col-form-label text-info"><b class="text-danger">* </b>Detalle de requerimiento:</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="detallerequerimiento1" name="detallerequerimiento1">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="fname"
                                   class="col-sm-3 text-end control-label col-form-label text-info"><b class="text-danger">* </b>Descripción de requerimiento:</label>
                            <div class="col-sm-9">
                                <textarea type="text" class="form-control" id="descripcionrequerimiento1" name="descripcionrequerimiento1" required="" placeholder="Subir el archivo correspondiente"></textarea>

                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="fname"
                                   class="col-sm-3 text-end control-label col-form-label text-info"><b class="text-danger">* </b>Tipo de dato:</label>
                            <div class="col-sm-9">
                                <select id="tipodato1" name="tipodato1" class="required form-control" required="">

                                    @foreach (var itemt1 in ViewBag.Tipodato)
                                    {
                                        <option value="@itemt1.IDtipodato">@itemt1.DescripcionTipoDato</option>
                                    }

                                </select>
                            </div>

                        </div>

                        <div class="form-group row">
                            <label for="fname"
                                   class="col-sm-3 text-end control-label col-form-label text-info"><b class="text-danger">* </b>Emisor:</label>


                            <div class="col-md-9">

                                <div class="form-check">
                                    <input type="radio" class="form-check-input"
                                           id="emisor1" name="emisor" required="" value="Administrador">
                                    <label class="form-check-label mb-0 text-secondary" for="customControlValidation1">
                                        Administrador
                                    </label>
                                </div>
                                <div class="form-check">
                                    <input type="radio" class="form-check-input"
                                           id="emisor2" name="emisor" required="" value="Autor">
                                    <label class="form-check-label mb-0 text-secondary" for="customControlValidation2">
                                        Autor
                                    </label>
                                </div>
                                <div class="form-check">
                                    <input type="radio" class="form-check-input"
                                           id="emisor3" name="emisor" required"" value="Evaluador">
                                    <label class="form-check-label mb-0 text-secondary" for="customControlValidation2">
                                        Evaluador
                                    </label>
                                </div>
                            </div>
                        </div>




                        <div class="form-group row">
                            <label for="fname"
                                   class="col-sm-3 text-end control-label col-form-label text-info"><b class="text-danger">* </b>Requiere respuesta de selección:</label>


                            <div class="col-md-9">

                                <div class="form-check">
                                    <input type="radio" class="form-check-input"
                                           id="respS" name="resp" value="Si" required>
                                    <label class="form-check-label mb-0 text-secondary" for="customControlValidation1">
                                        Si
                                    </label>
                                </div>
                                <div class="form-check">
                                    <input type="radio" class="form-check-input"
                                           id="respN" name="resp" value="No" required>
                                    <label class="form-check-label mb-0 text-secondary" for="customControlValidation2">
                                        No
                                    </label>
                                </div>

                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="fname"
                                   class="col-sm-3 text-end control-label col-form-label text-info"><b class="text-danger">* </b>Obligatorio:</label>


                            <div class="col-md-9">

                                <div class="form-check">
                                    <input type="radio" class="form-check-input"
                                           id="obligatorioS" name="obligatorio" value="true" required>
                                    <label class="form-check-label mb-0 text-secondary" for="customControlValidation1">
                                        Si
                                    </label>
                                </div>
                                <div class="form-check">
                                    <input type="radio" class="form-check-input"
                                           id="obligatorioN" name="obligatorio" value="false" required>
                                    <label class="form-check-label mb-0 text-secondary" for="customControlValidation2">
                                        No
                                    </label>
                                </div>

                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="fname"
                                   class="col-sm-3 text-end control-label col-form-label text-info"><b class="text-danger">* </b>Establecer como resultado de proceso:</label>


                            <div class="col-md-9">

                                <div class="form-check">
                                    <input type="radio" class="form-check-input"
                                           id="resultS" name="resultproceso" value="true" required>
                                    <label class="form-check-label mb-0 text-secondary" for="customControlValidation1">
                                        Si
                                    </label>
                                </div>
                                <div class="form-check">
                                    <input type="radio" class="form-check-input"
                                           id="resultN" name="resultproceso" value="false" required>
                                    <label class="form-check-label mb-0 text-secondary" for="customControlValidation2">
                                        No
                                    </label>
                                </div>

                            </div>
                        </div>




                        <div class="form-group row">
                            <label for="fname"
                                   class="col-md-3 text-end control-label col-form-label text-info"><b class="text-danger">* </b>Visible por:</label>
                        </div>

                        <div class="form-group row">
                            <div class="col-md-12">
                                <div class="row">
                                    <div class="col-md-2"></div>
                                    <div class="col-md-4">
                                        <label class="form-check-label mb-0 text-info col-md-6" for="customControlValidation1">
                                            Administrador
                                        </label>
                                        <div class="form-check">
                                            <input type="radio" class="form-check-input"
                                                   id="visibleadminS" name="visibleadmin" value="true" required>
                                            <label class="form-check-label mb-0 text-secondary" for="customControlValidation1">
                                                Si
                                            </label>
                                        </div>
                                        <div class="form-check">
                                            <input type="radio" class="form-check-input"
                                                   id="visibleadminN" name="visibleadmin" value="false" required>
                                            <label class="form-check-label mb-0 text-secondary" for="customControlValidation2">
                                                No
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <label class="form-check-label mb-0 text-info col-md-6" for="customControlValidation1">
                                            Autor
                                        </label>
                                        <div class="form-check">
                                            <input type="radio" class="form-check-input"
                                                   id="visibleautorS" name="visibleautor" value="true" required>
                                            <label class="form-check-label mb-0 text-secondary" for="customControlValidation1">
                                                Si
                                            </label>
                                        </div>
                                        <div class="form-check">
                                            <input type="radio" class="form-check-input"
                                                   id="visibleautorN" name="visibleautor" value="false" required>
                                            <label class="form-check-label mb-0 text-secondary" for="customControlValidation2">
                                                No
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <label class="form-check-label mb-0 text-info col-md-6" for="customControlValidation1">
                                            Evaluador
                                        </label>
                                        <div class="form-check">
                                            <input type="radio" class="form-check-input"
                                                   id="visibleES" name="visibleE" value="true" required>
                                            <label class="form-check-label mb-0 text-secondary" for="customControlValidation1">
                                                Si
                                            </label>
                                        </div>
                                        <div class="form-check">
                                            <input type="radio" class="form-check-input"
                                                   id="visibleEN" name="visibleE" value="false" required>
                                            <label class="form-check-label mb-0 text-secondary" for="customControlValidation2">
                                                No
                                            </label>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>



                    </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
                            <button type="submit" class="btn btn-info">Guardar</button>
                        </div>
                    }
                </div>
                <!--end formulario-->
            </div>

        </div>
    </div>
</div>





